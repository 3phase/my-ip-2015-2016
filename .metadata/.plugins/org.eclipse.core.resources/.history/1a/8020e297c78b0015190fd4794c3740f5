package org.elsysbg.ip.socket_project;

import java.util.HashMap;
import java.util.Map;

public class Register {

	private static Map<String, Integer> loggedUsers = new HashMap<String, Integer>();
	
	public Register() {
		// TODO Auto-generated constructor stub
	}
	
	public void increment_attendance() {
		
	}
	
	public int get_attendance(String username) {
		try {
			return loggedUsers.get(username);
		} catch(Exception e) {
			System.out.println("Log with this username is unavilable");
			return 1;
		}
	}
	
}
